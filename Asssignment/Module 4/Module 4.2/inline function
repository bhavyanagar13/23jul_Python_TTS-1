#include<iostream>
using namespace std;
class line
{
      public:
         inline float mul(float x,float y)
         {
            return x*y;
         }
         inline float cube(float x)
         {
            return x*x*x;
         }
};   
int main()
{
    line obj;
    float val1,val2;
    cout<<"Enter two value:"<<endl;
    cin>>val1>>val2;
    cout<<"Multiplication value is:"<<obj.mul(val1,val2)<<endl;
    cout<<"cube value is:"<<obj.cube(val1)<<"\n"<<obj.cube(val2);
    return 0;
}
